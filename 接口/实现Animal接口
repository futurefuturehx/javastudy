package study2;
//不是继承接口 而是实现接口 关键是实现
public class MammalInt implements Animal{
	//@Override是说明要重写方法 所以这边把implements删了会报错
	@Override
	public void eat() {
		System.out.println("Mammal eats");
	}
	@Override
	public void travel() {
		System.out.println("Mammal travels");
	}
	//可以有额外的自定义的函数
	public int noOfLegs() {
		return 0;
	}
	public static void main(String args[]) {
		//成为他才能调用他的功能 否则只是个模具
		MammalInt m=new MammalInt();
		m.eat();
		m.travel();
	}
}
